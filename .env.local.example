NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Optional: Service role key for admin operations (user management)
# Get this from Supabase Dashboard > Settings > API > service_role key
# WARNING: Never expose this in client-side code!
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Site URL for invitation emails (production URL)
# For Vercel: https://your-project.vercel.app
# For local development: http://localhost:3000
# If not set, will try to use VERCEL_URL or default to localhost
NEXT_PUBLIC_SITE_URL=https://your-production-domain.com

# Vipps payment recipient phone number (8 digits, no spaces)
# Example: 12345678
NEXT_PUBLIC_VIPPS_PHONE_NUMBER=your_vipps_phone_number
