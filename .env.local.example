NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Optional: Service role key for admin operations (user management)
# Get this from Supabase Dashboard > Settings > API > service_role key
# WARNING: Never expose this in client-side code!
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Vipps payment recipient phone number (8 digits, no spaces)
# Example: 12345678
NEXT_PUBLIC_VIPPS_PHONE_NUMBER=your_vipps_phone_number
